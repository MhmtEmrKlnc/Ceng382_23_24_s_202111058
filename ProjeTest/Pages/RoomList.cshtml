@page
@model RoomListModel
@{
    ViewData["Title"] = "Rooms";
}
<h1>@ViewData["Title"]</h1>

<table class="table mt-5">
    <thead>
        <tr>
            <th scope="col">Room Id</th>
            <th scope="col">Room Name</th>
            <th scope="col">Capacity</th>

        </tr>
    </thead>
    <tbody>
    @foreach (var room in RoomListModel.RoomList)
    {
        <tr>
            <td>@room.Id</td>
            <td>@room.RoomName</td>
            <td>@room.Capacity</td>

            
        </tr>
    }
    </tbody>
</table>


